<Window x:Class="User.View.TaskDescription"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:User.View"
        mc:Ignorable="d"
        Title="Объект оптимизации" Height="500" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox TextWrapping="Wrap" IsReadOnly="True" Text="{Binding Path=Getdescription, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</Window>
